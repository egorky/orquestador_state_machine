{
    "apis": [
        {
            "name": "fetch_cities_api",
            "endpoint": "http://127.0.0.1:3001/cities",
            "method": "GET",
            "headers": { "Content-Type": "application/json" }
        },
        {
            "name": "fetch_branches_api",
            "endpoint": "http://127.0.0.1:3001/branches",
            "method": "GET",
            "headers": { "Content-Type": "application/json" }
        },
        {
            "name": "fetch_specialities_api",
            "endpoint": "http://127.0.0.1:3001/specialities",
            "method": "GET",
            "headers": { "Content-Type": "application/json" }
        },
        {
            "name": "fetch_available_times_api",
            "endpoint": "http://127.0.0.1:3001/times",
            "method": "GET",
            "headers": { "Content-Type": "application/json" },
            "auth": {
                "type": "oauth2_client_credentials",
                "token_url": "http://127.0.0.1:3001/oauth/token",
                "client_id": "your_client_id",
                "client_secret": "your_client_secret",
                "scope": "read:times"
            }
        },
        {
            "name": "create_appointment_api",
            "endpoint": "http://127.0.0.1:3001/appointments",
            "method": "POST",
            "headers": { "Content-Type": "application/json" }
        }
    ]
}