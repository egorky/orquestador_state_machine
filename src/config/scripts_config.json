{
    "scripts": [
        {
            "name": "format_cities_list",
            "function_body": "return cities.map(c => c.city_name).join(', ');"
        },
        {
            "name": "format_branches_list",
            "function_body": "return branches.map(b => b.branch_name).join(', ');"
        },
        {
            "name": "format_specialities_list",
            "function_body": "return specialities.map(s => s.speciality_name).join(', ');"
        },
        {
            "name": "format_available_times_list",
            "function_body": "return available_times.join(', ');"
        }
    ]
}
